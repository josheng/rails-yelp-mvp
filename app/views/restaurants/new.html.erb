<h1>Add New Restaurant</h1>
<%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number, as: :string %>
    <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"], as: :select %>
    <%= f.submit %>
<% end %>
